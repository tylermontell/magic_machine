{
  "output": {
    "filePath": "repopack-output.xml",
    "style": "xml"
  },
  "remote": {
    "url": "https://github.com/tylermontell/magic_machine.git",
    "branch": "main"
  },
  "ignore": {
    "useGitignore": false,
    "useDefaultPatterns": true,
    "customPatterns": [
      "*.log",
      "*.swp", "*.swo", "*~",
      "node_modules/",
      "venv/", "venv_magic_machine/",
      ".DS_Store", "._*",
      ".vscode/",
      "build/", "dist/", "out/", "temp/", "tmp/",
      ".idea/",
      "*.pid", "*.seed", "*.class", "*.o", "*.out"
    ]
  }
}
